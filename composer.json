{
  "name": "coresys/user-management",
  "type": "bundle",
  "license": "MIT",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://CoreSys@bitbucket.org/jlcoresys/reverse-discriminator.git"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": "^7.2.5",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "antishov/doctrine-extensions-bundle": "^2.0@dev",
    "coresys/reverse-discriminator": "dev-master",
    "symfony/console": "5.1.*",
    "symfony/dotenv": "5.1.*",
    "symfony/flex": "^1.3.1",
    "symfony/framework-bundle": "5.1.*",
    "symfony/orm-pack": "^1.0",
    "symfony/security-bundle": "5.1.*",
    "symfony/yaml": "5.1.*"
  },
  "require-dev": {
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "CoreSys\\UserManagement\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CoreSys\\UserManagement\\Tests\\": "tests/"
    }
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": true,
      "require": "5.1.*"
    }
  }
}
