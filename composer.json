{
  "name": "coresys/user-management",
  "type": "bundle",
  "license": "MIT",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/JLCoreSys/ReverseDiscriminatorBundle.git"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^8.1",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "coresys/reverse-discriminator": "dev-main",
    "doctrine/doctrine-bundle": "^2.7",
    "doctrine/doctrine-fixtures-bundle": "^3.4",
    "doctrine/orm": "^2.13",
    "symfony/console": "6.2.*",
    "symfony/dotenv": "6.2.*",
    "symfony/flex": "^2",
    "symfony/form": "6.2.*",
    "symfony/security-bundle": "6.2.*",
    "symfony/yaml": "6.2.*"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true,
    "allow-plugins": {
      "symfony/flex": true
    }
  },
  "autoload": {
    "psr-4": {
      "CoreSys\\UserManagement\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CoreSys\\UserManagement\\Tests\\": "tests/"
    }
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": true,
      "require": "6.2.*"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "symfony/browser-kit": "6.2.*",
    "symfony/css-selector": "6.2.*",
    "symfony/phpunit-bridge": "^6.2"
  }
}
