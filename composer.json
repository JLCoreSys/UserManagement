{
  "name": "coresys/user-management",
  "type": "bundle",
  "license": "MIT",
  "repositories": [
    {
      "type": "vcs",
      "url": "git@bitbucket.org:jlcoresys/reverse-discriminator.git"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": "^8.1",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "antishov/doctrine-extensions-bundle": "^2.0@dev",
    "coresys/reverse-discriminator": "dev-master",
    "symfony/console": "6.1.*",
    "symfony/dotenv": "6.1.*",
    "symfony/flex": "^1.3.1",
    "symfony/framework-bundle": "6.1.*",
    "symfony/orm-pack": "^1.0",
    "symfony/security-bundle": "6.1.*",
    "symfony/yaml": "^6.1.*"
  },
  "require-dev": {
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "CoreSys\\UserManagement\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CoreSys\\UserManagement\\Tests\\": "tests/"
    }
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": true,
      "require": "6.1.*"
    }
  }
}
